  "license": "MIT",
  "type": "module",
  "keywords": [
    "mongo",
    "mongodb",
    "mongoose",
    "pagination",
    "cursor"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/men232/toolkit.git",
    "directory": "packages/mongo-pagination"
  },
  "scripts": {
    "build": "unbuild",
    "prepack": "unbuild",
    "test": "vitest run --typecheck",
